<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.sqz777.fishbowl.TimeList"
    android:orientation="vertical"
    android:background="@drawable/led_tl_bgcolor"
    android:weightSum="1">

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="-15dp"
        android:layout_marginLeft="-20dp"
        android:weightSum="1">

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;"
            android:id="@+id/back"
            android:layout_marginTop="0dp"
            android:background="@drawable/led_back_color"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:textSize="30dp"
            android:layout_weight="0.05" />

        <TextView
            android:layout_width="280dp"
            android:layout_height="55dp"
            android:text="LED_TimeList"
            android:id="@+id/textView"
            android:layout_marginTop="5dp"
            android:textSize="40dp"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:layout_marginLeft="10dp"
            />
    </LinearLayout>

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:weightSum="1"
        android:background="@drawable/led_edgecolor">

        <TextView
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:text="1點58分"
            android:id="@+id/showtime1"
            android:layout_marginTop="5dp"
            android:textSize="30sp"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:gravity="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:id="@+id/ib1"
            android:src="@drawable/led_on"
            android:background="#00000000"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        android:weightSum="1"
        android:background="@drawable/led_edgecolor" >

        <TextView
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:text="1點59分"
            android:id="@+id/showtime2"
            android:layout_marginTop="5dp"
            android:textSize="30dp"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:gravity="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:id="@+id/ib2"
            android:src="@drawable/led_off"
            android:background="#00000000"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        android:weightSum="1"
        android:background="@drawable/led_edgecolor">

        <TextView
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:text="未設定時間"
            android:id="@+id/showtime3"
            android:layout_marginTop="5dp"
            android:textSize="30dp"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:gravity="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:id="@+id/ib3"
            android:src="@drawable/led_on"
            android:background="#00000000"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        android:weightSum="1"
        android:background="@drawable/led_edgecolor" >

        <TextView
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:text="未設定時間"
            android:id="@+id/showtime4"
            android:layout_marginTop="5dp"
            android:textSize="30dp"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:gravity="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:id="@+id/ib4"
            android:src="@drawable/led_on"
            android:background="#00000000"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout tools:context="com.example.sqz777.fishbowl.TimeList"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        android:weightSum="1"
        android:background="@drawable/led_edgecolor" >

        <TextView
            android:layout_width="200dp"
            android:layout_height="fill_parent"
            android:text="未設定時間"
            android:id="@+id/showtime5"
            android:layout_marginTop="0dp"
            android:textSize="30dp"
            android:layout_marginLeft="10dp"
            android:textColor="#ffffff"
            android:gravity="center"/>

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:id="@+id/ib5"
            android:src="@drawable/led_on"
            android:background="#00000000"
            android:layout_weight="1" />

    </LinearLayout>

</LinearLayout>

